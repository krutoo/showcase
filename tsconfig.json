{
  "compilerOptions": {
    "resolveJsonModule": true,
    "noEmit": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "paths": {
      "#core": ["./src/core/index.ts"],
      "#runtime": ["./src/runtime/index.ts"]
    }
  }
}
